// Experiment by Tokyo Polytechnic University
// building height [m]
H    0.364;

// Block division
// along-wind coordinates
X0   #calc "-3.6*$H";
X1   #calc "10*$H";

// cross-wind coordinates
Y0   #calc "-2.2*$H";
Y1   #calc "2.2*$H";

// vertical coordinates
Z1   #calc "$H";
Z2   #calc "3.6*$H";

