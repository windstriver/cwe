FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 2.0847e-4;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 2.0847e-4;
    }

    outlet
    {
        type            inletOutlet;
        inletValue      uniform 0;
        value           uniform 0;
    }

    lowerWall
    {
        type            fixedValue;
        value           uniform 0;
    }

    upperWall
    {
        type            slip;
    }

    frontAndBack
    {
        type            slip;
    }

    roughnessBlocksGroup
    {
        type            fixedValue;
        value           uniform 0;
    }

}

