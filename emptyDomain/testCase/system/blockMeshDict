    ($X0 $Y0 $Z1)    // vertex 4
    ($X1 $Y0 $Z1)    // vertex 5
    ($X1 $Y1 $Z1)    // vertex 6
    ($X0 $Y1 $Z1)    // vertex 7

    ($X0 $Y0 $Z2)    // vertex 8
    ($X1 $Y0 $Z2)    // vertex 9
    ($X1 $Y1 $Z2)    // vertex 10
    ($X0 $Y1 $Z2)    // vertex 11

);

blocks
(
    // block 0
