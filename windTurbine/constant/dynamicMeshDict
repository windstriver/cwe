{
    cellZone        rotor;

    solidBodyMotionFunction  rotatingMotion;
    rotatingMotionCoeffs
    {
        origin      (1.510229221 0 1.672068885);
        axis        (0.993572 0 0.113203);
        omega       1.57 ; // rad/s  = 1rot per 4s
    }
}

