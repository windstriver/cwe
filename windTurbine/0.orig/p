FoamFile
{
    version     3.0;
    format      ascii;
    class       volScalarField;
    location    "0.org";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    inlet
    {
        type            zeroGradient;
    }

    outlet
    {
        type            fixedValue;
        value           uniform 0;
    }

    wall
    {
        type            zeroGradient;
    }

    top
    {
        type            slip;
    }

    right
    {
        type            slip;
    }
    
    left
    {
        type            slip;
    }
    cyclicAMI
    { type cyclicAMI;}
}


