FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

#include "../parameters"

application     pimpleFoam;

startFrom       latestTime;

startTime       0;

stopAt          endTime;

endTime         6.0;

deltaT          2e-03;

writeControl    timeStep;

writeInterval   50;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression uncompressed;

timeFormat      scientific;

timePrecision   6;

runTimeModifiable yes;

adjustTimeStep    yes;

maxCo             0.5;

maxDeltaT         2.0e-3;

functions
{
    #includeFunc probes
    #includeFunc singleGraphInlet
    #includeFunc singleGraphBuilding
}

