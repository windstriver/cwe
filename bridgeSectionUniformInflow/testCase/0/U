FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    inlet
    {
        type            turbulentInlet;
        referenceField  uniform (7.5 0 0);
        fluctuationScale (0.0951 0.0748 0.0814);
        value           uniform (7.5 0 0);
    }

    outlet
    {
        type            pressureInletOutletVelocity;
        value           uniform (0 0 0);
    }

    upperAndLowerWall
    {
        type            slip;
    }

    frontAndBack
    {
        type            symmetry;
    }
}

