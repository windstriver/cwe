{
    inlet
    {
        type            fixedValue;
        value           uniform 0;
    }

    cyclicLeft
    {
        type            fixedValue;
        value           uniform 0;
    }

    groundLeft
    {
        type            fixedValue;
        value           uniform 0.04375;
    }

    topLeft
    {
        type            symmetry;
        value           uniform 0;
    }

    frontAndBackLeft
    {
        type            symmetry;
        value           uniform 0;
    }

    cyclicRight
    {
        type            fixedValue;
        value           uniform 0;
    }

    outlet
    {
        type            fixedValue;
        value           uniform 0;
    }

    groundRight
    {
        type            fixedValue;
        value           uniform 0.04375;
    }

    topRight
    {
        type            symmetry;
        value           uniform 0;
    }

    frontAndBackRight
    {
        type            symmetry;
        value           uniform 0;
    }

}

