FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    inlet
    {
        type            zeroGradient;
    }

    cyclicLeft
    {
        type            cyclic;
    }

    groundLeft
    {
        type            zeroGradient;
    }

    topLeft
    {
        type            symmetry;
    }

    frontAndBackLeft
    {
        type            symmetry;
    }

    cyclicRight
    {
        type            cyclic;
    }

    outlet
    {
        type            totalPressure;
        rho             none;
        p0              uniform 0;
        value           uniform 0;
    }

    groundRight
    {
        type            zeroGradient;
    }

    topRight
    {
        type            symmetry;
    }

    frontAndBackRight
    {
        type            symmetry;
    }

}

